{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "Book",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "author": {
      "type": "string"
    },
    "pages": {
      "type": "integer"
    },
    "releasedate": {
      "type": "date"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "books"
    },
    "rating": {
      "type": "integer",
      "max": 10,
      "min": 0
    },
    "userrating": {
      "type": "component",
      "repeatable": true,
      "component": "user-rating.user-rating"
    }
  }
}
